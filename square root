class Solution {
  public:
    int floorSqrt(int n) {
        // code here
        int low=1;
        int high=n;
        while(low<=high){
            long mid=(low+high)/2;
            long val=mid*mid;
            if(val<=long(n)){
                low=(int)(mid+1);
            }
            else{
                high=(int)(mid-1);
            }
        }
        return high;
    }
};
